all:
	$(MAKE) -C basic
	$(MAKE) -C benchmarks
	$(MAKE) -C demos
	$(MAKE) -C nehe

clean:
	$(MAKE) -C basic clean
	$(MAKE) -C benchmarks clean
	$(MAKE) -C demos clean
	$(MAKE) -C nehe clean
		
dist:
	$(MAKE) -C basic dist
	$(MAKE) -C benchmarks dist
	$(MAKE) -C demos dist
	$(MAKE) -C nehe dist


