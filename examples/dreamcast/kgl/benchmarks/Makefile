all:
	$(KOS_MAKE) -C quadmark
	$(KOS_MAKE) -C trimark

clean:
	$(KOS_MAKE) -C quadmark clean
	$(KOS_MAKE) -C trimark clean
		
dist:
	$(KOS_MAKE) -C quadmark dist
	$(KOS_MAKE) -C trimark dist


